{
  "routes": [
    {
      "route": "/animals/*",
      "serve": "/animals/*",
      "statusCode": 200
    },
    {
      "route": "/static/*",
      "serve": "/static/*",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/animals/*", "/static/*"]
  },
  "mimeTypes": {
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif",
    ".svg": "image/svg+xml",
    ".mp3": "audio/mpeg",
    ".wav": "audio/wav",
    ".mp4": "video/mp4",
    ".webm": "video/webm"
  }
}
